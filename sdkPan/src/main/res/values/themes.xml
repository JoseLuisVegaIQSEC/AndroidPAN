    <style name="SDKIQSECTimerText">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">@dimen/dimen8</item>
        <item name="android:background">@drawable/timer_background</item>
        <item name="android:paddingStart">@dimen/dimen8</item>
        <item name="android:paddingEnd">@dimen/dimen8</item>
        <item name="android:paddingTop">@dimen/dimen6</item>
        <item name="android:paddingBottom">@dimen/dimen6</item>
        <item name="android:textColor">@color/sdk_wfc_white</item>>
