    <style name="sdk_pan_spinner">
        <item name="android:textAppearance">@style/sdk_pan_text_s3_blue</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:background">@drawable/sdk_pan_spinner_gradient</item>
        <item name="android:popupBackground">@drawable/abc_popup_background_mtrl_mult</item>
        <item name="android:dropDownSelector">?attr/listChoiceBackgroundIndicator</item>
        <item name="android:dropDownVerticalOffset">0dip</item>
        <item name="android:dropDownHorizontalOffset">0dip</item>
        <item name="android:dropDownWidth">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">left|start|center_vertical</item>
        <item name="overlapAnchor">true</item>
    </style>
